<?xml version="1.0" encoding="utf-8"?>
<window id="106">
	<defaultcontrol>1000</defaultcontrol>
	<animation type="WindowOpen">
		<effect type="fade" start="0" end="100" time="150" delay="50"/>
		<effect type="zoom" start="90" end="100" center="0,454" time="200"/>
	</animation>
	<animation type="WindowClose">
		<effect type="fade" start="100" end="0" time="150" delay="50"/>
		<effect type="zoom" start="100" end="90" center="0,454" time="200"/>
	</animation>
	<coordinates>
		<system>1</system>
		<left>0</left>
		<top>0</top>
	</coordinates>
	<controls>
		<control type="image" id="999">
			<description>background image</description>
			<left>10</left>
			<top>-25</top>
			<width>530</width>
			<height>760</height>
			<texture border="5">dialogs\dialog-background-select.png</texture>
		</control>
		<control type="grouplist" id="996">
			<description>grouplist for context buttons</description>
			<left>50</left>
			<top>24</top>
			<width>450</width>
			<height max="660">auto</height>
			<itemgap>2</itemgap>
		</control>
		<control type="button">
			<description>Close Window button</description>
			<left>260</left>
			<top>5</top>
			<width>64</width>
			<height>32</height>
			<label>-</label>
			<font>-</font>
			<onclick>PreviousMenu</onclick>
			<texturefocus>DialogCloseButton-focus.png</texturefocus>
			<texturenofocus>DialogCloseButton.png</texturenofocus>
			<onleft>2</onleft>
			<onright>2</onright>
			<onup>2</onup>
			<ondown>2</ondown>
			<visible>system.getbool(input.enablemouse)</visible>
		</control>
		<control type="button" id="1000">
			<description>button template</description>
			<left>-</left>
			<top>-</top>
			<width>450</width>
			<height>50</height>
			<font>Medium</font>
			<align>center</align>
			<textcolor>Text</textcolor>
			<disabledcolor>DisabledText</disabledcolor>
			<texturefocus border="4,5,6,8">controls\button_f.png</texturefocus>
			<texturenofocus border="4,5,6,8">controls\button_nf.png</texturenofocus>
		</control>
		<include>Debug</include>
	</controls>
</window>
